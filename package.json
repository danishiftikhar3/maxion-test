    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "ejs": "^3.1.10",
    "express": "^4.17.3",
    "firebase-admin": "^12.1.1",
    "geojson": "^0.5.0",
    "googleapis": "^140.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "install": "^0.13.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loops": "^3.4.0",
    "mailersend": "^2.2.0",
    "mathjs": "^14.0.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.13",
    "npm": "^10.5.0",
