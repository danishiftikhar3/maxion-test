{
  "name": "App-api",
  "version": "1.0.0",
  "description": "RESTful APIs for App",
  "scripts": {
    "dev": "cross-env NODE_PATH=./src ts-node-dev --respawn ./src/index.ts",
    "build": "rimraf -rf dist && dotenv -e .env -- node -e \"console.log(process.env.REDIS_ENV)\" | xargs ./flush_redis_keys.sh && cross-env tsc",
    "start": "cross-env NODE_ENV=production NODE_PATH=./dist ts-node ./dist/index.js",
    "lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "lint:fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "migrate": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:run --config ./src/database/config.ts",
    "migrate:create": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:create --config ./src/database/config.ts -n",
    "migrate:revert": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:revert --config ./src/database/config.ts",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean:install": "pnpm i --frozen-lockfile",
    "clean-cache": "dotenv -e .env -- node -e \"console.log(process.env.REDIS_ENV)\" | xargs ./flush_redis_keys.sh && cross-env "
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.722.0",
    "@sendgrid/mail": "^8.1.3",
    "@turf/turf": "^7.0.0",
