    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "mkdirp": "^3.0.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "10.7.0",
