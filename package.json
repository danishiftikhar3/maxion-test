    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.0.0",
    "typescript-transform-paths": "^3.4.7",
    "validator": "^13.7.0",
    "wellknown": "^0.5.0",
    "winston": "^3.12.0"
  },
